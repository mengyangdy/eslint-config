{
  "name": "@dylanjs/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "DylanJS's eslint and prettier",
  "license": "MIT",
  "homepage": "",
  "repository": {},
  "bugs": {},
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "prettier"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --fix",
    "publish-pkg": "npm publish --access public",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "cm": "git add . && dy git-commit"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "eslint-config-flat-gitignore": "^0.3.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0"
  },
  "devDependencies": {
    "@dylanjs/cli": "^0.1.0",
    "@types/node": "^22.5.5",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
