{
  "name": "@dylanjs/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "DylanJS's eslint and prettier",
  "license": "MIT",
  "homepage": "",
  "repository": {},
  "bugs": {},
  "keywords": ["eslint", "eslint-config", "eslint-config-vue", "prettier"],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc --noEmit --skipLibCheck &&tsup",
    "cm": "git add . && dy git-commit",
    "lint": "eslint . --fix",
    "publish-pkg": "npm publish --access public"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^0.4.1",
    "@eslint/js": "^9.10.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-json-sort": "^0.0.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@dylanjs/cli": "^0.1.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.5.5",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.10.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
